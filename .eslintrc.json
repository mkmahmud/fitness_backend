{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint-config-prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "import", "prettier"],

  "rules": {
    "import/extensions": "off"
  }
}
